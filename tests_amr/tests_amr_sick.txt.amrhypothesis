# ::id 0
# ::snt There is no boy playing outdoors and there is no man smiling .
(a / and
      :op1 (p / play-01
            :polarity -
            :ARG0 (b / boy)
            :location (o / outdoors))
      :op2 (s / smile-01
            :polarity -
            :ARG0 (m / man)))

# ::id 1
